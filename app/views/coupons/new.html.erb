<h1>Coupon Form</h1>
<%= form_tag coupons_path do %>

      <h1><%= label_tag 'store' %>
      <%= text_field_tag :'coupon[store]'%>
      </h1>

      <h1><%= label_tag 'coupon_code' %>
      <%= text_area_tag :'coupon[coupon_code]'%>
    </h1>

      <%= submit_tag "Submit Coupon" %>

  <% end %>
